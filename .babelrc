{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": "last 10 versions, > 1%, ie >= 9, Android >= 4.4, iOS >= 8",
          "node": "current"
        },
        "modules": false,
        "useBuiltIns": "entry"
      }
    ]
  ],
  "sourceType": "unambiguous", // 严格区分commonJS文件和ES6文件
  "ignore": ["node_modules"]
}
